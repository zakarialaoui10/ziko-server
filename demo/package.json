{
    "name": "ssr-test",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "node src/server",
        "build": "npm run build:client && npm run build:server",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
        "preview": "cross-env NODE_ENV=production node src/server",
        "dev-v": "vite",
        "build-v": "npm run build:client-v && npm run build:server-v",
        "build:client-v": "vite build --outDir dist-v/client",
        "build:server-v": "vite build --ssr src/entry-server.js --outDir dist-v/server",
        "preview-v": "node dist-v/server/entry-server.js"
    },
    "dependencies": {
        "vite": "^6.0.7",
        "ziko-server": "^0.9.4"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "vite-plugin-ssr": "^0.4.85"
    }
}
